{
  "name": "@ixocreate/admin-frontend",
  "version": "2.0.0",
  "description": "IXOCREATE admin frontend",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --proxy-config proxy.config.json --extra-webpack-config webpack.partial.js",
    "build": "ng build --prod --extra-webpack-config webpack.partial.js --output-path=build/ && rm build/index.html",
    "build-with-stats": "ng build --prod --extra-webpack-config webpack.partial.js --output-path=build/ --stats-json && rm build/index.html && yarn run bundle-report",
    "bundle-report": "webpack-bundle-analyzer build/stats.json",
    "test": "ng test",
    "test:once": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci": "npm run lint && npm run test:once && npm run e2e"
  },
  "private": true,
